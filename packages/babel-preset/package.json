{
  "name": "@vectorspace/babel-preset",
  "version": "0.0.0",
  "description": "Default babel configuration for @vectorspace packages.",
  "license": "MIT",
  "author": "VectorSpace <engineering@vectorspace.io>",
  "homepage": "https://github.com/vector-space/vectorspace",
